IDR_MAINFRAME MENU
BEGIN
    POPUP "파일(&F)"
    BEGIN
        MENUITEM "새로 만들기(&N)\tCtrl+N",                ID_FILE_NEW
        MENUITEM "열기(&O)...\tCtrl+O",            ID_FILE_OPEN
        MENUITEM "저장(&S)\tCtrl+S",               ID_FILE_SAVE
